{
  "name": "vsex001",
  "displayName": "VS Extension Example 001",
  "description": "サンプルVS Code拡張機能",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsex001.helloWorld",
    "onCommand:vsex001.openDatabaseClient"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsex001.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vsex001.openDatabaseClient",
        "title": "Database Client: Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run copy-i18n",
    "copy-i18n": "mkdir -p out/i18n && cp src/i18n/*.json out/i18n/",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/pg": "^8.16.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "eslint": "^8.x",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "mysql2": "^3.16.0",
    "pg": "^8.16.3"
  }
}
